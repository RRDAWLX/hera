{
  "name": "hera-weweb",
  "version": "1.1.5",
  "description": "wx 2 app",
  "main": "bin/weweb",
  "bin": {
    "hweweb": "bin/weweb"
  },
  "scripts": {
    "start": "npm run build && ./bin/weweb demo",
    "build": "NODE_ENV=production webpack --progress",
    "build:dev": "webpack --progress",
    "dev": "WATCH=true webpack --config webpack.config.js --progress",
    "precommit": "lint-staged",
    "format":
      "prettier-standard './*.js' && prettier-standard 'src/**/*.js' && prettier-standard 'lib/**/*.js'",
    "lint":
      "eslint src/ --ignore-pattern src/service/ --ignore-pattern src/view/behaviors --ignore-pattern src/view/components --ignore-pattern src/template",
    "generate-docs":
      "node_modules/.bin/jsdoc --configure .jsdoc.json && open-cli jsdocs/index.html"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/weidian-inc/hera.git"
  },
  "lint-staged": {
    "{*.js,*weweb,*.json}": ["prettier-standard", "git add"]
  },
  "dependencies": {
    "archiver": "^2.1.0",
    "boxen": "^1.3.0",
    "chalk": "^2.3.0",
    "commander": "^2.12.2",
    "concat-with-sourcemaps": "^1.0.4",
    "convert-source-map": "^1.3.0",
    "et-improve": "^0.1.3",
    "fs-extra": "^5.0.0",
    "glob": "^7.1.2",
    "merge": "^1.2.0",
    "minimatch": "^3.0.2",
    "mkdir-p": "0.0.7",
    "node-parallel": "^0.1.6",
    "node-serial": "^0.1.1",
    "object-assign": "^4.1.0",
    "promisify-node": "^0.4.0",
    "semver-compare": "^1.0.0",
    "source-map": "^0.5.6",
    "uglify-js": "^3.3.2",
    "whatwg-fetch": "^2.0.3",
    "wxml-transpiler": "^1.0.5",
    "wxss-transpiler": "^1.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/eslint-parser": "^7.13.14",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.14.0",
    "@babel/runtime": "^7.14.0",
    "actionsheet": "^0.1.2",
    "autoscale-canvas": "0.0.3",
    "babel-loader": "^8.2.2",
    "classnames": "^2.2.5",
    "component-classes": "^1.2.6",
    "component-closest": "^1.0.1",
    "component-emitter": "^1.2.1",
    "component-event": "^0.2.0",
    "component-events": "^1.0.10",
    "component-file-picker": "^0.2.1",
    "component-query": "0.0.3",
    "component-querystring": "^2.0.0",
    "component-raf": "^1.2.0",
    "component-tap-event": "^1.1.4",
    "component-tween": "^1.2.0",
    "component-upload": "^0.3.0",
    "css-loader": "^0.28.7",
    "domify": "^1.4.0",
    "ei-loader": "^0.1.1",
    "eslint": "^7.25.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-loader": "^0.4.4",
    "husky": "^0.14.3",
    "jr-qrcode": "^1.0.5",
    "jsdoc": "^3.6.6",
    "json-loader": "^0.5.4",
    "lint-staged": "^6.0.0",
    "nodemon": "^1.11.0",
    "nprogress": "^0.2.0",
    "open-cli": "^6.0.1",
    "pinch-zoom": "^0.3.8",
    "prettier-standard": "^8.0.0",
    "prop-detect": "^1.1.0",
    "radio-component": "0.0.2",
    "request-component": "0.0.4",
    "style-loader": "^0.19.0",
    "tui-jsdoc-template": "^1.2.2",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.9.1"
  },
  "browser": {
    "emitter": "component-emitter",
    "querystring": "component-querystring",
    "event": "component-event",
    "tap-event": "component-tap-event",
    "closest": "component-closest",
    "tween": "component-tween",
    "raf": "component-raf",
    "classes": "component-classes",
    "file-picker": "component-file-picker",
    "upload": "component-upload",
    "query": "component-query",
    "events": "component-events",
    "request": "request-component"
  },
  "license": "BSD-2-Clause"
}
